    "import json\n",
    "\n",
    "text = \"\"\n",
    "filename = \"./conll2003/train.txt\"\n",
    "with open(filename, 'r') as file:\n",
    "    while True:\n",
    "        temp = file.readline().split(\" \")[0]\n",
    "        if not temp:\n",
    "            break\n",
    "        text += \" \" + temp\n",
    "\n",
    "print(text)"
